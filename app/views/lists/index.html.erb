<h1>Lists</h1>

<%# <%= cl_image_tag("THE_IMAGE_ID_FROM_LIBRARY",
      width: 400, height: 300, crop: :fill) %>




<%@lists.each do |list| %>
  <div>
    <%=link_to list.name, list_path(list) %>
    <% if list.photo.attached? %>
      <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill  %>
    <% end %>
  </div>
<%end %>


<p><%= link_to "add a list!", new_list_path %></p>
